package com.raman.Spring_AOP;

public class CustomerService {
	
	private String name;
	private String url;
	
	public void getName() {
		System.out.println("customer name : " + this.name);
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	
	

}
