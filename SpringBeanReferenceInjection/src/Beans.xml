<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
  <bean id = "address" class = "AddressCollection">
  <property name="addressList">
  <list>
  <ref bean = "address1"></ref>
  <ref bean = "address2"></ref>
  <value>United States</value>
  </list>
  </property>
  
  <property name="addressSet">
  <set>
  <ref bean = "address1"></ref>
  <ref bean = "address2"></ref>
  <value>United States</value>
  </set>
  </property>
  
  
  <property name="adressMap">
  <map>
<entry key = "1" value ="INDIA" />
<entry key = "2" value ="United States" />
<entry key = "3" value ="Canada" />
<entry key = "4" value ="Pakistan" />
</map>
 </property>
  
  
<property name="addressProp">
 <props>
<prop key = "one" >INDIA" </prop>
<prop key = "one" >INDIA" </prop>
<prop key = "two" >United States</prop>
<prop key = "three"> Canada </prop>
<prop key = "four" >Pakistan</prop>
</props>
</property>
</bean>
  
  </beans>